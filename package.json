{
  "name": "@wkovacs64/prettier-config",
  "version": "0.0.0-semantically-released",
  "description": "@wKovacs64 Prettier config",
  "keywords": [
    "prettier",
    "prettierconfig",
    "wkovacs64"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "lib",
    "scripts",
    "index.js"
  ],
  "homepage": "https://github.com/wKovacs64/prettier-config#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/prettier-config.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/prettier-config/issues"
  },
  "scripts": {
    "cm": "git-cz",
    "format": "prettier --write \"**/*.{js,ts,tsx,html,json,md,yml,yaml}\"",
    "lint": "eslint --ext \".js,.ts,.tsx\" .",
    "postinstall": "node ./scripts/postinstall.js",
    "validate": "npm-run-all --silent --parallel lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 8.9.0"
  },
  "dependencies": {
    "cpy": "7.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "commitizen": "4.0.3",
    "cross-env": "6.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.6.0",
    "eslint-plugin-wkovacs64": "2.1.1",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "semantic-release": "15.13.28"
  }
}
