  "private": false,
  "prettier": "./index.js",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 18"
  },
  "packageManager": "pnpm@10.30.1",
  "dependencies": {
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.2",
    "@changesets/cli": "2.29.8",
    "@wkovacs64/eslint-config": "7.13.0",
    "cross-env": "10.1.0",
    "eslint": "9.39.3",
    "prettier": "3.8.1"
