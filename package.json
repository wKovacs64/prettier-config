{
  "name": "@wkovacs64/prettier-config",
  "version": "0.0.0-semantically-released",
  "description": "@wKovacs64 Prettier config",
  "keywords": [
    "prettier",
    "prettierconfig",
    "wkovacs64"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/wKovacs64/prettier-config#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/prettier-config.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/prettier-config/issues"
  },
  "scripts": {
    "cm": "git-cz",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "validate": "npm-run-all --silent --parallel lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 8.9.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "commitizen": "4.2.1",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.8.1",
    "eslint-plugin-wkovacs64": "8.0.0",
    "husky": "4.2.5",
    "lint-staged": "10.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.1",
    "semantic-release": "17.1.1"
  }
}
